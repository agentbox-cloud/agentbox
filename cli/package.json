{
  "name": "@agentbox/cli",
  "version": "1.4.3",
  "description": "CLI for managing agentbox sandbox templates",
  "homepage": "https://agentbox.space",
  "license": "MIT",
  "author": {
    "name": "AgentBox",
    "email": "hello@agentbox.dev",
    "url": "https://agentbox.space"
  },
  "bugs": "https://github.com/agentbox-dev/agentbox/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentbox-dev/agentbox",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "agentbox",
    "ai-agents",
    "agents",
    "ai",
    "code-interpreter",
    "sandbox",
    "code",
    "cli",
    "runtime",
    "vm",
    "nodejs",
    "javascript",
    "typescript"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsc --noEmit --skipLibCheck && tsup --minify",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "pnpm build && cd testground/demo-basic && ../../dist/index.js template build",
    "check-deps": "knip",
    "update-deps": "ncu -u && pnpm i",
    "generate-ref": "./scripts/generate_sdk_ref.sh"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/command-exists": "^1.2.3",
    "@types/inquirer": "^9.0.7",
    "@types/json2md": "^1.5.4",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^18.18.6",
    "@types/prompts": "^2.4.9",
    "@types/ssh2": "^1.15.5",
    "@types/update-notifier": "6.0.5",
    "archiver": "^7.0.1",
    "json2md": "^2.0.1",
    "knip": "^5.43.6",
    "npm-check-updates": "^16.14.6",
    "tsup": "^8.4.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "LICENSE",
    "README",
    "package.json"
  ],
  "bin": {
    "agentbox-js-sdk": "dist/index.js"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "async-listen": "^3.0.1",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "command-exists": "^1.2.9",
    "commander": "^11.1.0",
    "console-table-printer": "^2.11.2",
    "agentbox-js-sdk": "workspace:*",
    "inquirer": "^9.2.12",
    "minimatch": "^10.0.3",
    "open": "^9.1.0",
    "prompts": "2",
    "ssh2": "^1.16.0",
    "strip-ansi": "^7.1.0",
    "update-notifier": "^6.0.2",
    "yup": "^1.3.2"
  },
  "engines": {
    "node": ">=18"
  }
}
